package test;

import java.util.Scanner;

public class Test {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		int  sum = 0;
		for(int i = 0; i <= n; i++){
			while(i != 0){
				int p = i%10;
				sum += p;
				i = i/10;
			}
		}
		
		System.out.println(sum);
	}
}
	